{
  "name": "@colin30/web-keyword-multiplier",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "repository": "git@github.com:colin30/core.git",
  "author": "colin30 <colin30@post.com>",
  "private": true,
  "prettier": "@colin30/configs/prettier",
  "config": {
    "name": "colin30-web-keyword-multiplier",
    "bucket": "colin30-web-keyword-multiplier-static",
    "profile": "default",
    "template_title": "Colin30 Content Marketing Keyword Multiplier"
  },
  "scripts": {
    "start": "lite-server -c config/lite-server.js",
    "watch": "NODE_ENV=development webpack-cli --config config/webpack-dev.js --progress --watch",
    "build": "NODE_ENV=production webpack-cli --config config/webpack-pro.js --progress",
    "mb": "aws s3 mb s3://$npm_package_config_bucket --profile $npm_package_config_profile",
    "sync": "aws s3 sync static s3://$npm_package_config_bucket --acl public-read --profile $npm_package_config_profile",
    "deploy": "yarn build && yarn sync"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@colin30/configs": "0.0.1",
    "@colin30/web-shared": "0.0.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "file-loader": "^6.0.0",
    "gsap": "^3.5.0",
    "html-webpack-plugin": "^4.3.0",
    "lite-server": "^2.5.4",
    "lodash": "^4.17.20",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intersection-observer": "^8.27.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-form-saga": "^0.2.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.2"
  }
}
