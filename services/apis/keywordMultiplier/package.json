{
  "name": "@colin30/services-apis-keywordMultiplier",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "repository": "git@github.com:colin30/core.git",
  "author": "colin30 <colin30@post.com>",
  "private": true,
  "prettier": "@colin30/configs/prettier",
  "config": {
    "name": "colin30-services-apis-keywordMultiplier",
    "host": "localhost",
    "port": "2000",
    "profile": "default"
  },
  "scripts": {
    "start": "sam local start-api --host $npm_package_config_host --port $npm_package_config_port",
    "mb": "aws s3 mb s3://$npm_package_config_name --profile $npm_package_config_profile",
    "package": "sam package --template-file template.yaml --s3-bucket $npm_package_config_name --output-template-file packaged.yaml --profile $npm_package_config_profile",
    "deploy": "sam deploy --template-file packaged.yaml --stack-name $npm_package_config_name --capabilities CAPABILITY_IAM --profile $npm_package_config_profile",
    "delete": "aws cloudformation delete-stack --stack-name $npm_package_config_name --profile $npm_package_config_profile"
  },
  "devDependencies": {
    "@colin30/configs": "0.0.1"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.8",
    "express": "^4.17.1",
    "mongoose": "^5.10.2"
  }
}
