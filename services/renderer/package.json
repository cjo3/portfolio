{
  "name": "@colin30/services-renderer",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "repository": "git@github.com:colin30/core.git",
  "author": "colin30 <colin30@post.com>",
  "private": true,
  "prettier": "@colin30/configs/prettier",
  "config": {
    "region": "localhost",
    "endpoint": "http://localhost:8000",
    "profile": "default",
    "tableName": "colin30-services-renders"
  },
  "scripts": {
    "createTable": "node -e 'require(\"@colin30/shared/helpers/dynamoTableManager\").createTable(require(\"./tableSchemas\")[0])'",
    "deleteTable": "node -e 'require(\"@colin30/shared/helpers/dynamoTableManager\").deleteTable(require(\"./tableSchemas\")[0].TableName)'",
    "listTables": "node -e 'require(\"@colin30/shared/helpers/dynamoTableManager\").listTables()'",
    "resetTables": "yarn deleteTables && yarn createTables"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@colin30/configs": "0.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-source-map-support": "^2.1.3",
    "dotenv": "^8.2.0",
    "serverless-domain-manager": "^4.2.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-offline": "^6.7.0",
    "serverless-webpack": "^5.3.4",
    "webpack": "^4.44.1",
    "webpack-node-externals": "^2.5.2",
    "aws-sdk": "^2.745.0"
  },
  "dependencies": {
    "@colin30/shared": "0.0.1",
    "uuid": "^8.3.0"
  }
}
