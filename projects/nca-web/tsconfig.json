{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "es2020",
    "jsx": "react",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "lib": ["es6", "dom"],
    "noImplicitThis": true,
    "strictNullChecks": true
  }
}
