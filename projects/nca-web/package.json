{
  "name": "@cjo3/nca-web",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "author": "cjo3",
  "private": true,
  "scripts": {
    "lite": "lite-server -c lite-server.js",
    "content": "node src/content/index.js",
    "start": "BUILD_ENV=development webpack-dev-server --config webpack.config.js",
    "build": "BUILD_ENV=staging  webpack --config webpack.config.js --progress",
    "renderer": "BUILD_ENV=staging webpack --config renderer/webpack.config.js --progress",
    "nodemon": "nodemon distRenderer/renderer.js",
    "sync:content": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").sync(require(\"./cliParams\").content)'",
    "sync:assets": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").sync(require(\"./cliParams\").assets)'",
    "sync:bundles": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").sync(require(\"./cliParams\").bundles)'",
    "sync:preRenders": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").sync(require(\"./cliParams\").preRenders)'",
    "invalidate": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").invalidate(require(\"./cliParams\").invalidate)'",
    "clear": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").rm(require(\"./cliParams\").clear)'",
    "rebuild": "yarn build && yarn content && yarn renderer && yarn nodemon",
    "deploy": "yarn invalidate && yarn sync:assets && yarn sync:bundles && yarn sync:content && yarn sync:preRenders"
  },
  "dependencies": {
    "@cjo3/shared": "~0.0.1",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "axios": "~0.21.0",
    "clsx": "~1.1.1",
    "file-saver": "~2.0.2",
    "formik": "~2.2.3",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "react-ga": "~3.3.0",
    "react-loadable": "~5.5.0",
    "react-router-dom": "~5.2.0",
    "recaptcha-v3": "~1.8.0",
    "redux": "~4.0.5",
    "regenerator-runtime": "~0.13.7",
    "yup": "~0.29.3"
  },
  "devDependencies": {
    "@babel/core": "~7.12.3",
    "@babel/preset-typescript": "~7.12.7",
    "@types/material-ui": "~0.21.8",
    "@types/react": "~16.9.55",
    "@types/react-dom": "~16.9.9",
    "@types/react-loadable": "~5.5.4",
    "@types/react-router-dom": "~5.1.6",
    "@types/yup": "~0.29.9",
    "@typescript-eslint/eslint-plugin": "~4.6.1",
    "@typescript-eslint/parser": "~4.6.1",
    "babel-loader": "~8.1.0",
    "babel-plugin-transform-remove-console": "~6.9.4",
    "clean-webpack-plugin": "~3.0.0",
    "colors": "~1.4.0",
    "dotenv": "~8.2.0",
    "eslint": "~7.12.1",
    "eslint-config-prettier": "~6.15.0",
    "eslint-plugin-prettier": "~3.1.4",
    "eslint-plugin-react": "~7.21.5",
    "file-loader": "~6.2.0",
    "html-webpack-plugin": "~4.5.0",
    "imagemin-webp-webpack-plugin": "~3.3.3",
    "nodemon": "~2.0.6",
    "prettier": "~2.1.2",
    "pug": "~3.0.0",
    "pug-loader": "~2.4.0",
    "redux-logger": "~3.0.6",
    "ts-loader": "~8.0.11",
    "typescript": "~4.0.5",
    "webpack": "~4.44.2",
    "webpack-bundle-analyzer": "~4.1.0",
    "webpack-dev-server": "~3.11.0"
  }
}
