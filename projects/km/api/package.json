{
  "name": "@cjo3/projects-km-api",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "repository": "git@github.com:cjo3/primary.git",
  "author": "cjo3",
  "private": true,
  "prettier": "@cjo3/configs/prettier",
  "scripts": {
    "dev": "NODE_ENV=development sls offline",
    "sta": "NODE_ENV=staging sls offline",
    "pro": "NODE_ENV=production sls offline",
    "deploy:dev": "NODE_ENV=development sls deploy",
    "deploy:sta": "NODE_ENV=staging sls deploy",
    "deploy:pro": "NODE_ENV=production sls deploy",
    "createTable": "node -e 'require(\"@cjo3/shared/raw/dynamoDbTableManager\").createTable(require(\"./tableSchema\"))'",
    "deleteTable": "node -e 'require(\"@cjo3/shared/raw/dynamoDbTableManager\").deleteTable(require(\"./tableSchema\").TableName)'",
    "listTables": "node -e 'require(\"@cjo3/shared/raw/dynamoDbTableManager\").listTables()'",
    "resetTable": "yarn deleteTable && yarn createTable"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@cjo3/configs": "0.0.1",
    "@cjo3/shared": "0.0.1",
    "aws-sdk": "^2.745.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-source-map-support": "^2.1.3",
    "dotenv": "^8.2.0",
    "js-yaml": "^3.14.0",
    "serverless-domain-manager": "^4.2.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-offline": "^6.7.0",
    "serverless-webpack": "^5.3.4",
    "stripe": "^8.107.0",
    "webpack": "^4.44.1",
    "webpack-node-externals": "^2.5.2",
    "axios": "^0.20.0"
  }
}
