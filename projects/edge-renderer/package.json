  "license": "MIT",
  "private": true,
  "prettier": "@cjo3/configs/prettier",
  "scripts": {
    "deploy": "NODE_ENV=production sls deploy",
    "undeploy": "NODE_ENV=production sls remove",
    "update": "NODE_ENV=production sls deploy --update-config",
