    "aws-sdk": "^2.745.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-source-map-support": "^2.1.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-webpack": "^5.3.4"
  },
  "dependencies": {
    "html-minifier": "^4.0.0"
  }
