{
  "name": "@cjo3/dle-web",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "repository": "git@github.com:cjo3/primary.git",
  "author": "cjo3",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js --progress",
    "build:staging": "NODE_ENV=staging webpack --config webpack.config.js --progress",
    "lite": "lite-server -c lite-server.js",
    "preRender": "NODE_ENV=production webpack --config ssr/webpack.config --progress",
    "preRender:staging": "NODE_ENV=staging webpack --config ssr/webpack.config --progress",
    "render": "NODE_ENV=production node -e 'require(\"./ssr/renderer\").paths(require(\"./ssr/locals\").paths)'",
    "render:staging": "NODE_ENV=staging node -e 'require(\"./ssr/renderer\").paths(require(\"./ssr/locals\").paths)'",
    "sync:assets": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").sync(require(\"./cliParams\").assets)'",
    "sync:bundles": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").sync(require(\"./cliParams\").bundles)'",
    "sync:renders": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").sync(require(\"./cliParams\").renders)'",
    "invalidate": "node -e 'require(\"@cjo3/shared/raw/s3Manager\").invalidate(require(\"./cliParams\").invalidate)'",
    "clear": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").rm(require(\"./cliParams\").clear)'",
    "deploy": "yarn invalidate && yarn clear && yarn build && yarn sync:assets && yarn sync:bundles && yarn preRender && yarn render && yarn sync:renders",
    "test": "jest --watch --detectOpenHandles",
    "snapshots": "jest --updateSnapshot --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@cjo3/shared": "~0.0.1",
    "@material-ui/lab": "~4.0.0-alpha.56",
    "@stripe/react-stripe-js": "~1.1.2",
