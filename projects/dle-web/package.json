    "react-redux": "~7.2.2",
    "react-router-dom": "~5.2.0",
    "react-scroll": "~1.8.1",
    "redux": "~4.0.5",
    "regenerator-runtime": "~0.13.7",
    "xlsx": "~0.16.8",
    "yup": "~0.29.3"
  },
  "devDependencies": {
