{
  "name": "@cjo3/km-web",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "author": "cjo3",
  "private": true,
  "prettier": "@cjo3/configs/prettier",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build:staging": "NODE_ENV=staging webpack --config webpack.config.js --progress",
    "build": "NODE_ENV=production webpack --config webpack.config.js --progress",
    "build:renderer": "SRC_ENV=staging webpack --config renderer/webpack.config.js --progress",
    "watch:renderer": "webpack --config renderer/webpack.config.js --progress --watch",
    "start:renderer": "nodemon distRenderer/renderer.js",
    "lite": "lite-server -c lite-server.js",
    "sync:assets": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").sync(require(\"./cliParams\").assets)'",
    "sync:bundles": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").sync(require(\"./cliParams\").bundles)'",
    "sync:preRenders": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").sync(require(\"./cliParams\").preRenders)'",
    "invalidate": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").invalidate(require(\"./cliParams\").invalidate)'",
    "clear": "NODE_ENV=staging node -e 'require(\"@cjo3/shared/raw/s3Manager\").rm(require(\"./cliParams\").clear)'"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "import-sort-style-module-scoped",
      "options": {}
    }
  },
  "dependencies": {
    "@cjo3/shared": "~0.0.1",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/system": "~4.9.14",
    "@stripe/react-stripe-js": "~1.1.2",
    "@stripe/stripe-js": "~1.10.0",
    "axios": "~0.21.0",
    "clsx": "~1.1.1",
    "formik": "~2.2.3",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "react-ga": "~3.3.0",
    "react-loadable": "~5.5.0",
    "react-redux": "~7.2.2",
    "react-router-dom": "~5.2.0",
    "redux": "~4.0.5",
    "redux-saga": "~1.1.3"
  },
  "devDependencies": {
    "@babel/core": "~7.12.3",
    "@babel/plugin-transform-runtime": "~7.12.1",
    "@cjo3/configs": "~0.0.1",
    "babel-loader": "~8.1.0",
    "babel-plugin-transform-remove-console": "~6.9.4",
    "clean-webpack-plugin": "~3.0.0",
    "colors": "~1.4.0",
    "dotenv": "~8.2.0",
    "eslint": "~7.12.1",
    "eslint-config-prettier": "~6.15.0",
    "eslint-plugin-jest-dom": "~3.2.4",
    "eslint-plugin-no-loops": "~0.3.0",
    "eslint-plugin-prettier": "~3.1.4",
    "eslint-plugin-react": "~7.21.5",
    "file-loader": "~6.2.0",
    "html-minifier": "~4.0.0",
    "html-webpack-plugin": "~4.5.0",
    "imagemin-webp-webpack-plugin": "~3.3.3",
    "jest": "~26.6.3",
    "prettier": "~2.1.2",
    "pug": "~3.0.0",
    "pug-loader": "~2.4.0",
    "redux-logger": "~3.0.6",
    "regenerator-runtime": "~0.13.7",
    "webpack": "~4.44.2",
    "webpack-dev-server": "~3.11.0",
    "webpack-node-externals": "~2.5.2"
  }
}
