{
  "name": "@cjo3/edge-fetcher",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:cjo3/primary.git",
  "author": "cjo3",
  "license": "MIT",
  "private": true,
  "prettier": "@cjo3/configs/prettier",
  "scripts": {
    "debug": "NODE_ENV=development SLS_DEBUG=* sls offline",
    "dev": "NODE_ENV=development sls offline start --port",
    "pro": "NODE_ENV=production sls offline",
    "deploy:dev": "NODE_ENV=development sls deploy",
    "deploy:pro": "NODE_ENV=production sls deploy",
    "undeploy:pro": "NODE_ENV=production sls remove",
    "update": "NODE_ENV=production sls deploy --update-config",
    "event": "NODE_ENV=production sls invoke local -f viewerRequest -p v1/events/non-www.json",
    "invalidate": "node -e 'require(\"@cjo3/shared/raw/s3Manager\").invalidate(require(\"./cliParams\").invalidate)'"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@cjo3/configs": "0.0.1",
    "@cjo3/shared": "0.0.1",
    "aws-sdk": "^2.745.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-source-map-support": "^2.1.3",
    "colors": "^1.4.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-webpack": "^5.3.4"
  }
}
