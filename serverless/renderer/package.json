{
  "name": "@colin30/serverless-renderer",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "repository": "git@github.com:colin30/core.git",
  "author": "colin30 <colin30@post.com>",
  "private": true,
  "prettier": "@colin30/configs/prettier",
  "config": {
    "region": "localhost",
    "endpoint": "http://localhost:8000",
    "profile": "default",
    "tableName": "colin30-renders"
  },
  "scripts": {
    "dynamo": "cd ~/dynamodb && java -Djava.library.path=./DynamoDBLocal_lib -jar DynamoDBLocal.jar -sharedDb",
    "itemsCreateOne": "sls invoke local -f createOne -p events/createOne.json",
    "itemsGetAll": "sls invoke local -f getAll -p events/getAll.json",
    "itemsUpdateOne": "sls invoke local -f updateOne -p events/updateOne.json",
    "tablesCreate": "node -e 'require(\"@colin30/serverless-shared/localTableManager\").createTable(require(\"./tableSchemas\")[0])'",
    "tablesDelete": "node -e 'require(\"@colin30/serverless-shared/localTableManager\").deleteTable(require(\"./tableSchemas\")[0].TableName)'",
    "tablesList": "node -e 'require(\"@colin30/serverless-shared/localTableManager\").listTables()'"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@colin30/configs": "0.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-source-map-support": "^2.1.3",
    "dotenv": "^8.2.0",
    "serverless-domain-manager": "^4.2.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-offline": "^6.7.0",
    "serverless-webpack": "^5.3.4",
    "webpack": "^4.44.1",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@colin30/serverless-shared": "0.0.1",
    "aws-sdk": "^2.745.0",
    "uuid": "^8.3.0"
  }
}
