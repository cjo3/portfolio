{
  "name": "@colin30/clients-web-colin30",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "repository": "git@github.com:colin30/core.git",
  "author": "colin30 <colin30@post.com>",
  "private": true,
  "prettier": "@colin30/configs/prettier",
  "config": {
    "name": "colin30-clients-web-colin30",
    "bucket": "colin30-clients-web-colin30-static",
    "profile": "default",
    "template_title": "Colin30 JavaScript App Development"
  },
  "scripts": {
    "start": "lite-server -c config/lite-server.js",
    "watch": "NODE_ENV=development webpack-cli --config config/webpack-dev.js --progress --watch",
    "build": "NODE_ENV=production webpack-cli --config config/webpack-pro.js --progress",
    "mb": "aws s3 mb s3://$npm_package_config_bucket --profile $npm_package_config_profile",
    "sync": "aws s3 sync static s3://$npm_package_config_bucket --acl public-read --profile $npm_package_config_profile",
    "deploy": "yarn build && yarn sync"
  },
  "devDependencies": {
    "@colin30/configs": "0.0.1",
    "@colin30/clients-web-shared": "0.0.1",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "lite-server": "^2.5.4",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
